{
  "name": "@zaifer/nuxt-query",
  "version": "1.0.0",
  "author": "zaifer",
  "description": "Simple asyncData wrapper",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zaif3r/nuxt-query.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.1.1",
    "@vueuse/core": "^9.12.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.1",
    "@nuxt/test-utils": "^3.1.1",
    "changelogen": "^0.4.1",
    "eslint": "^8.32.0",
    "nuxt": "^3.1.1",
    "vitest": "^0.28.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
